<?xml version="1.0" encoding="UTF-8"?>
<launch>
    <arg name="cmd" default="$(find xacro)/xacro $(find clover_description)/urdf/drones/clover_base.xacro"/>
    <param command="$(arg cmd)" name="camera_description"/>
    <node name="$(anon spawn)" output="screen" pkg="gazebo_ros" type="spawn_model" args="-urdf -param camera_description -model clover"/> <!-- missing flag:  -package_to_model -->
</launch>
