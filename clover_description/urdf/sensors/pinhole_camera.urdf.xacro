  <xacro:include filename="$(find clover_description)/urdf/camera_sensor.urdf.xacro"/>
  <camera_sensor name="main_camera" width="640" height="480" rate="30" horizontal_fov="${120.0*M_PI/180.0}"/>
</robot>
